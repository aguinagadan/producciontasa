<!DOCTYPE html>
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Document
        link(rel="stylesheet", href="css/stylesheet.css")
    body
        .game-tasa
            .contanier-game-load
                video(id="video",src="tasa.mp4",controls="true" )
                //- <iframe width="853" height="480" src="https://www.youtube.com/embed/v5C8uDynIH8" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
            #app-mapa.container-game
                .header
                    .millas {{millas}} MILLAS NAUTICAS
                    .right
                        .title AVANZA Y GANA MILLAS NAUTICAS.
                        .subtitle Las millas nauticas son puntos en el aula virtual.
                .mapa
                    .boya(v-for="item in cursos",class="boya",:class="{'active': item.status,'actual': item.actual}")
                        .actual(v-if="item.active")
                        .img
                        .point {{item.title}}
                    .boatch
            .back-game
                .person-tasa
                .content
                    .title GANASTE
                    .millas +50 MILLAS NAUTICAS
                    .btn
                        button(onclick="close_back()") continuar
            .final
                .luz
                .cofre
                    .tapa
                    .caja
                .text ganaste 350 millas nauticas
        script(src="https://cdn.jsdelivr.net/npm/vue@2.5.16/dist/vue.js")
        script(src="https://code.jquery.com/jquery-3.5.1.min.js", integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=",crossorigin="anonymous")
        script(src="js/load.js")
        script(src="js/app.js")